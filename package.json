    "dev": "npm run clean && tsc -w",
    "lint": "tslint --project tsconfig.json --format stylish",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "omg-console": "^1.1.2",
    "tslib": "1.9.3"
  },
  "devDependencies": {
    "@types/node": "11.9.5",
    "rimraf": "2.6.3",
    "tslint": "5.13.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.3.3333"
  }
}
